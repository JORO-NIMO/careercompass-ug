{
  "name": "opportunity-backend",
  "version": "1.0.0",
  "description": "RSS-based opportunity ingestion backend with AI semantic search",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "ingest": "tsx src/scripts/ingest.ts",
    "ingest:single": "tsx src/scripts/ingest-single.ts",
    "serve": "tsx src/api/server.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src"
  },
  "keywords": [
    "rss",
    "opportunities",
    "jobs",
    "scholarships",
    "supabase",
    "embeddings",
    "semantic-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "country-list": "^2.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "he": "^1.2.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "openai": "^4.28.0",
    "rss-parser": "^3.13.0",
    "url-parse": "^1.5.10",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/he": "^1.2.3",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/url-parse": "^1.4.11",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.55.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
